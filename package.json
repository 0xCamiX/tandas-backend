{
	"name": "tandas-backend",
	"module": "src/server.ts",
	"type": "module",
	"private": true,
	"prisma": {
		"seed": "bun run prisma/seed.ts"
	},
	"scripts": {
		"dev": "bun run --watch src/server.ts",
		"postinstall": "bunx prisma generate",
		"build": "bunx prisma generate && bun build ./src/server.ts --outdir ./dist --target bun --minify",
		"start": "bun run dist/server.js",
		"lint": "bunx biome check --write",
		"check": "bunx biome ci",
		"db:seed": "bun run prisma/seed.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@types/bun": "latest",
		"@types/cors": "^2.8.19",
		"@types/node-cron": "^3.0.11",
		"prisma": "^7.1.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@prisma/adapter-pg": "^7.1.0",
		"@prisma/client": "^7.1.0",
		"@prisma/extension-accelerate": "^2.0.2",
		"@types/express": "^5.0.5",
		"@types/morgan": "^1.9.10",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"better-auth": "^1.3.34",
		"cors": "^2.8.5",
		"express": "^5.1.0",
		"morgan": "^1.10.1",
		"node-cron": "^4.2.1",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"valibot": "^1.1.0"
	}
}