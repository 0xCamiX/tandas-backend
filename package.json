	},
	"dependencies": {
		"@prisma/client": "^6.19.0",
		"@prisma/extension-accelerate": "^2.0.2",
		"@types/express": "^5.0.5",
		"@types/morgan": "^1.9.10",
		"better-auth": "^1.3.34",
		"express": "^5.1.0",
		"morgan": "^1.10.1"
	}
